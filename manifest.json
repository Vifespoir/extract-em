{
  "manifest_version": 2,
  "name": "Extract 'Em!",
  "description": "Extracts attachments from messages under the selected folder or account.",
  "version": "1.0.2",
  "author": "tmccoid.tech",
  "browser_specific_settings": {
    "gecko": {
      "id": "extractem@tmccoid.tech",
      "strict_min_version": "102.0"
    }
  },
  "icons": {
    "64": "icons/extractem-64px.png",
    "32": "icons/extractem-32px.png",
    "16": "icons/extractem-16px.png"
},
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "/options/options.html",
    "open_in_tab": false,
    "browser_style": true
  },
  "permissions": [
    "messagesRead",
    "accountsRead",
    "menus",
    "menus.overrideContext",
    "storage",
    "downloads"
  ]
}